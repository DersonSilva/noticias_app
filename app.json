{
  "expo": {
    "name": "noticias_app",
    "slug": "noticias_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/expo-images/icon-android.png",
    "scheme": "noticiasapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/expo-images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#7A5CFA"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.anderson.noticiasapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/expo-images/icon-android.png",
        "backgroundColor": "#7A5CFA"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/expo-images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 36,
            "targetSdkVersion": 36,
            "buildToolsVersion": "36.0.0"
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "GNEWS_API_KEY": "74c53a7ac3e2e772efbc4f3f723d0a2f",
      "GNEWS_BASE_URL": "https://gnews.io/api/v4",
      "router": {},
      "eas": {
        "projectId": "2d511818-2162-4e8c-8fdd-bfefeb4ca103"
      }
    }
  }
}
