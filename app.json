{
  "expo": {
    "name": "noticias_app",
    "slug": "noticias_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/expo-images/icon-android.png",
    "scheme": "noticiasapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/expo-images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#7A5CFA"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/expo-images/adaptive-icon.png",
        "backgroundColor": "#7A5CFA"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/expo-images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
